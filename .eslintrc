{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "semi": ["error", "never"],
        "quotes": [2, "single"],
        "indent": ["error", 4],
        "max-len": ["error", { "code": 80 }]
    }
  }